<?xml version="1.0" encoding="utf-8" ?>

<root>
    <animSet image = "player_body_default" path = "assets\images\player">
        <anim name = "idle">0</anim>
        <anim name = "horz_move">0t3</anim>
        <anim name = "vert_move">4t7</anim>
        <anim name = "whip" looping = "false" fps = "16">8h2,9t10,11h2,12</anim>
        <anim name = "whip_vert" looping = "false" fps = "16">4t7</anim>
        <anim name = "grapple">13t16t10</anim>
        <anim name = "exit" looping = "false">17t23</anim>
        <anim name = "fall">24</anim>
        <anim name = "land">25</anim>
        <anim name = "hit">25</anim>
        <anim name = "die" looping = "false">26t28</anim>
    </animSet>

    <animSet image = "player_head_default" path = "assets\images\player">
        <anim name = "front">0</anim>
        <anim name = "front_squash">1</anim>
        <anim name = "back">2</anim>
        <anim name = "back_squash">3</anim>
        <anim name = "side">4</anim>
        <anim name = "whip_front">5</anim>
        <anim name = "grapple">6t8r10</anim>
        <anim name = "horz_move">9t12</anim>
    </animSet>

    <animSet image = "whip" path = "assets\images\player">
        <anim name = "reset">0</anim>
        <anim name = "whip" looping = "false" fps = "14">0h2,1t2</anim>
    </animSet>
</root>

